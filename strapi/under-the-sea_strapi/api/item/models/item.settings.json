{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "name": "Item"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "url": {
      "type": "uid",
      "targetField": "name"
    },
    "price": {
      "type": "integer",
      "required": true
    }
  }
}
